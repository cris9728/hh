pipeline
{
agent any

stages{
     stage('compile satge')
     {
        steps{
           withMaven(maven : 'maven_3_6_0'){
              sh 'mvn clean compile'
}
}
}
}
  stage('testing stage')
{
      steps{
       withMaven(maven : 'maven_3_6_0'){
              sh 'mvn testing'
}
}
}
stage('deploy stage')
{
      steps{
       withMaven(maven : 'maven_3_6_0'){
              sh 'mvn deploy'
}
}
}
}
